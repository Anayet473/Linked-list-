#include<bits/stdc++.h>
using namespace std;
struct Node{
    int data;
    Node*next;
    Node*prev;
};
int main(){
Node*a=new Node();
Node*b=new Node();
Node*c=new Node();
a->data=10;
b->data=20;
c->data=30;
a->next=b;
a->prev=NULL;
b->next=c;
b->prev=a;
c->next=NULL;
c->prev=b;
Node*head=a;

while(head!=NULL){
    cout<<head->data<<" ";
head=head->next;

}
cout<<"\n";
Node* tail=c;

while(tail!=head){
    cout<<tail->data<<" ";
    tail=tail->prev;
}
delete a;
delete b;
delete c;




    return 0;

}
